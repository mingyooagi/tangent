{
  "name": "next-plugin-tangent",
  "version": "0.0.1",
  "description": "Next.js plugin for Tangent - Visual Tuner for AI-generated code",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./api": {
      "types": "./dist/api.d.ts",
      "import": "./dist/api.js",
      "require": "./dist/api.cjs"
    },
    "./loader": {
      "types": "./dist/loader.d.ts",
      "import": "./dist/loader.js",
      "require": "./dist/loader.cjs"
    }
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mingyouagi/tangent.git"
  },
  "homepage": "https://github.com/mingyouagi/tangent#readme",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "next": "^13.0.0 || ^14.0.0 || ^15.0.0"
  },
  "dependencies": {
    "tangent-transform": "workspace:*",
    "magicast": "^0.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "next": "^14.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "webpack": "^5.89.0"
  },
  "license": "MIT"
}
