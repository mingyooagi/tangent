{
  "name": "vite-plugin-tangent",
  "version": "0.0.1",
  "description": "Vite plugin for Tangent - Visual Tuner for AI-generated code",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0"
  },
  "dependencies": {
    "magicast": "^0.3.0",
    "tangent-transform": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.4.0"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "tangent",
    "visual-tuner",
    "ai-code",
    "live-editing"
  ],
  "license": "MIT"
}
